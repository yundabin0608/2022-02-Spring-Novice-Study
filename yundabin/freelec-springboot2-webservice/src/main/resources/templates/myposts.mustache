{{>layout/header}}
<a href="/mypage" role="button" class="btn btn-secondary" style="text-decoration:none; color: black">마이페이지로 가기</a>
<table class="table table-horizontal table-bordered">
    <thead class="thead-strong">
    <tr>
        <th>게시글 번호</th>
        <th>제목</th>
        <th>최종 수정일</th>
    </tr>
    </thead>
    <tbody id="tbody">
    {{#posts}}
        <tr>
            <td>{{id}}</td>
            <td><a href="/posts/detail/{{id}}">{{title}}</a></td>
            <td>{{modifiedDate}}</td>
        </tr>
    {{/posts}}
    </tbody>
</table>

{{>layout/footer}}